//洛谷P4378
#include<bits/stdc++.h>
using namespace std;

int main()
{
	map<int,int> m;
	map<int,int> m_f;
	map<int,int>::iterator v;
	int N,i,r=0;
	cin>>N;
	for(i=0;i<N;i++)
	{
		int x;
		cin>>x;
		m[x]=i;
		m_f[x]++;
		}
	for(v=m.begin(),i=0;v!=m.end();v++,i++)
	{
		i+=m_f[v->first]-1;
		if(v->second-i>r)
		r=v->second-i;
		}
	cout<<r+1;
}

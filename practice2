//洛谷R58457606
#include<bits/stdc++.h>
using namespace std;

int n,m,s,t,min_= 999999;
bool o[11]= {};

class QV
{
public:
	multimap<int,int> link;
	int id= 0;
}qv[10001];

void find(QV &q,int W)
{
	o[q.id]= 1;
	if(q.id==t)
	{
		if(min_>W)
		{
			min_= W;
			o[q.id]= 0;
			return;
		}
		else
		{
			o[q.id]= 0;
			return;
		}
	}
	for(multimap<int,int>::iterator v= q.link.begin();v!=q.link.end();v++)
		if(o[v->first]==0)
		{	
			int W_f= W;
			if(W<v->second)	
				W= v->second; 
			find(qv[v->first],W);
			W= W_f;
		}
	o[q.id]= 0;
	return;
}

int main()
{
	
	cin>>n>>m>>s>>t;
	for(int i= 1;i<=m;i++)
	{
		int u,v,w;
		cin>>u>>v>>w;
		qv[u].link.insert({v,w});
		qv[v].link.insert({u,w});
		qv[u].id= u;
		qv[v].id= v;
	}
	find(qv[s],0);
	cout<<min_;
}

//洛谷P1387
#include<bits/stdc++.h>
using namespace std;

int main()
{
	int n, m,ans= 0;
	int a[101][101] = {}, b[101][101] = {};
	cin >> n >> m;
	for(int i= 1;i<=n;i++)
		for (int j = 1;j <=m;j++)
		{
			cin >> a[i][j];
			if (a[i][j] == 1)
			{
				b[i][j] = min(min(b[i][j - 1], b[i - 1][j]),b[i-1][j-1]) + 1;
				if (ans < b[i][j])
					ans = b[i][j];
			}
		}
	cout << ans;
}

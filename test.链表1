//初始化链表 
#include<bits/stdc++.h>
using namespace std;

struct link
{
	int ele;
	link *next;
};

link *SetLink(int Long)
{
	link *head= new link();
	link *q= head;
	for(int i= 1;i<=Long;i++)
	{
		link *p= new link();
		p->next= NULL;
		q->next= p;
		q= p;
	}
	return head;
}

int main()
{
	SetLink(5);
}
